{
  "name": "scroll-sync",
  "main": "./lib/scroll-sync",
  "version": "0.2.0",
  "description": "Synchronize scrolling of two editor panes by content.",
  "consumedServices": {
    "status-bar": {
      "versions": {
        "^1.0.0": "consumeStatusBar"
      }
    }
  },
  "activationCommands": {
    "atom-text-editor": ["scroll-sync:toggle"]
  },
  "repository": "https://github.com/mark-hahn/scroll-sync",
  "license": "MIT",
  "engines": {
    "atom": ">0.170.0"
  },
  "dependencies": {
    "diff_match_patch": "0.1.x",
    "status-bar": "^2.0.3"
  }
}
